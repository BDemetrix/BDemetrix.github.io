				[
					{title: '  ', start_title: ' ', command: 'FIND F ', syntax: 'FIND F ', params: /^F\s(.*)$/i, ac: /^F\s([^@]+)$/i, act:'wiki', url: '/search/results/?q=$1&in=articles', tooltip: ' '},
