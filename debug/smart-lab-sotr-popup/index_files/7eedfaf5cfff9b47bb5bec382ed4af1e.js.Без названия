i.val("");},onCloseComplete:function(){this.destroy();}});e.open();}window.abuse=d;}b(document).ready(function(){if(typeof(CURRENT_USER_ID)!=="undefined"){a();
}});})(jQuery);
(function(a){a(document).ready(function(){window.ban=function(h){var f=a("#user_ban_dialog");var g={id:"userBanDialog",title:" ",confirmButton:"",cancelButton:"",content:f,confirm:function(){var i=this;
